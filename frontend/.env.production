# API Configuration
# These will be replaced by Netlify environment variables during build
VITE_API_URL=${API_URL}
VITE_WS_URL=${WS_URL}

# Security Configuration
VITE_ENABLE_RATE_LIMITING=true
VITE_ENABLE_CIRCUIT_BREAKING=true

# AI Configuration
VITE_MAX_AI_REQUESTS_PER_MINUTE=10
VITE_AI_FAILURE_THRESHOLD=3
VITE_AI_RESET_TIMEOUT_MS=30000
